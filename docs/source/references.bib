@Article{Bodesheim23,
author={Bodesheim, David
and Biele, Robert
and Cuniberti, Gianaurelio},
title={Hierarchies of {H}ofstadter butterflies in 2D covalent organic frameworks},
journal={npj 2D Materials and Applications},
year={2023},
month={Mar},
day={25},
volume={7},
number={1},
pages={16},
abstract={The Hofstadter butterfly is one of the first and most fascinating examples of the fractal and self-similar quantum nature of free electrons in a lattice pierced by a perpendicular magnetic field. However, the direct experimental verification of this effect on single-layer materials is still missing as very strong and inaccessible magnetic fields are necessary. For this reason, its indirect experimental verification has only been realized in artificial periodic 2D systems, like moir{\'e} lattices. The only recently synthesized 2D covalent organic frameworks might circumvent this limitation: Due to their large pore structures, magnetic fields needed to detect most features of the Hofstadter butterfly are indeed accessible with today technology. This work opens the door to make this exotic and theoretical issue from the 70s measurable and might solve the quest for the experimental verification of the Hofstadter butterfly in single-layer materials. Moreover, the intrinsic hierarchy of different pore sizes in 2D covalent organic framework adds additional complexity and beauty to the original butterflies and leads to a direct accessible playground for new physical observations.},
issn={2397-7132},
doi={10.1038/s41699-023-00378-0},
url={https://doi.org/10.1038/s41699-023-00378-0}
}

